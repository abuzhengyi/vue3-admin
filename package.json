    "preview:production": "npm run build:production && vite preview",
    "lint:eslint": "eslint src --ext .vue,.js,.ts,.jsx,.tsx --fix",
    "lint:stylelint": "stylelint **/*.{css,less,scss,vue} --fix",
    "lint:prettier": "prettier --write **/*.{js,ts,json,tsx,css,less,scss,vue}",
    "lint": "npm run lint:eslint && npm run lint:stylelint && npm run lint:prettier"
  },
